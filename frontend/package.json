{
  "name": "arcade",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ignore-path .eslintignore --ext js,ts,jsx,tsx --fix",
    "lint-check": "eslint . --ignore-path .eslintignore --ext js,ts,jsx,tsx",
    "format": "prettier --write \"./**/*.{css,scss,json,md,js,ts,tsx,jsx}\"",
    "format-check": "prettier --check \"./**/*.{css,scss,json,md,js,ts,tsx,jsx}\"",
    "scss-lint": "stylelint \"./**/*.{css,scss}\"",
    "scss-lint-fix": "stylelint \"./**/*.{css,scss}\" --fix",
    "preview": "vite preview"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.13.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react": "^4.0.1",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^36.0.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "react-icons": "^4.10.1",
    "tailwindcss": "^3.3.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2",
    "vite": "^4.3.9"
  }
}
